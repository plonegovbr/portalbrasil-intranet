{
  "name": "@plonegovbr/portalbrasil-intranet",
  "version": "1.0.0-alpha.4",
  "description": "Solução de Intranet construída com Plone",
  "main": "src/index.js",
  "license": "MIT",
  "keywords": [
    "volto-addon",
    "volto",
    "plone",
    "plonegovbr",
    "intranet",
    "react"
  ],
  "author": "PloneGov-BR",
  "homepage": "https://github.com/plonegovbr/portalbrasil-intranet#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:plonegovbr/portalbrasil-intranet.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "i18n": "rm -rf build/messages && NODE_ENV=production i18n --addon",
    "dry-release": "release-it --dry-run",
