    "release-major-alpha": "release-it major --preRelease=alpha",
    "release-alpha": "release-it --preRelease=alpha"
  },
  "addons": [
    "@plone-collective/volto-bookmarks",
    "@mbarde/volto-image-crop-widget",
    "@eeacms/volto-pdf-block",
    "@plonegovbr/volto-vlibras",
    "@kitconcept/volto-light-theme"
  ],
  "dependencies": {
    "@plone/components": "workspace:*",
    "@eeacms/volto-pdf-block": "1.0.12",
    "@mbarde/volto-image-crop-widget": "0.5.1",
    "@kitconcept/volto-light-theme": "5.0.2",
    "@plone-collective/volto-bookmarks": "2.0.1",
    "@plonegovbr/volto-vlibras": "0.4.0"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@plone/scripts": "^3.6.1",
    "release-it": "^17.1.1"
  }
